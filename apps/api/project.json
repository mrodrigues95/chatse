{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/api",
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": ["{workspaceRoot}/dist/apps/api", "{workspaceRoot}/dist/intermediates/apps/api"],
      "options": {
        "configuration": "Debug",
        "noDependencies": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "serve": {
      "executor": "@nx-dotnet/core:serve",
      "options": {
        "configuration": "Debug"
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
    },
    "swagger": {
      "executor": "@nx-dotnet/core:update-swagger",
      "outputs": ["{workspaceRoot}/options.output"],
      "options": {
        "output": "libs/generated/api-swagger/swagger.json"
      },
      "dependsOn": ["build"]
    },
    "migrations-add": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet ef migrations add",
        "cwd": "apps/api"
      }
    },
    "migrations-remove": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet ef migrations remove",
        "cwd": "apps/api"
      }
    },
    "migrations-update": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet ef database update",
        "cwd": "apps/api"
      }
    }
  },
  "tags": []
}
